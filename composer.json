{
    "name": "love-dj/payment",
    "type": "library",
    "description": "支付宝支付、微信支付、方便快速接入，最完整的开源支付 php sdk",
    "keywords": ["alipay", "weixin", "支付宝支付", "微信支付", "集成支付接口SDK"],
    "homepage": "http://helei112g.github.io/payment",
    "license": "MIT",
    "authors": [
        {
            "name": "Leo",
            "email": "dayugog@gmail.com",
            "homepage": "https://dayutalk.cn"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {"Payment\\": "src/"}
    },
    "extra": {
        "payment": {
            "config": {
                "payment": "src/config/payment.php"
            }
        }
    },
    "require-dev": {
        "codeception/codeception": "*"
    }
}
